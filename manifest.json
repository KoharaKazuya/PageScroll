{
    "name": "Page Scroll",
    "version": "0.1.1",
    "manifest_version": 2,

    "description": "スクロール後の画面が見やすくなるようにスクロール用を調節します",
    "content_scripts": [
    {
        "matches": ["file:///*/*", "http://*/*", "https://*/*"],
        "js": ["jquery-2.0.3.min.js", "scroll.js"],
        "all_frames": true
    }
    ]
}
